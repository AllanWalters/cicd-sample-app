{
    "executionRoleArn": "arn:aws:iam::513461903764:role/ECSTaskExecutionRoleU5",
    "containerDefinitions": [
        {
            "name": "API",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ],
            "environment": [
                {
                    "name": "spring.datasource.url",
                    "value": "jdbc:postgresql://database-1-instance-1.abc123.us-west-2.rds.amazonaws.com/users"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/sample-appFamily",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs",
                    "awslogs-create-group": "true"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "sample-appFamily"
}